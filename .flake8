[flake8]
max-line-length = 120
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    .pytest_cache,
    .mypy_cache,
    build,
    dist,
    target,
    dbt_packages,
    site-packages,
    *.egg-info,
    .eggs
per-file-ignores =
    __init__.py:F401
